    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.3",
